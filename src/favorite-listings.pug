doctype
html(lang='en')
    head
        meta(name='viewport')
        meta(content='width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0')
        meta(http-equiv="X-UA-Compatible")
        title= 'Favorite Listings'
    body
        header.header
            .header_top
                .container
                    .row
                        .col-xs-4
                        .col-xs-8
                            nav
                                ul.main-menu
                                    each val in ['Buy', 'Sell', 'Rent', 'Landlords', 'About us']
                                        li
                                            a= val
    section.s-favorite-listings
        .container
            .row
                .col-md-12
                    div#v-favorite-listings
    footer